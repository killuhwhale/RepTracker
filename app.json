{
  "expo": {
    "name": "RepTracker",
    "slug": "RepTracker",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "platforms": [
      "android",
      "ios"
    ],
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "buildNumber": "1",
      "bundleIdentifier": "com.killuhwhale.reptracker",
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash.png"
      },
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {}
        },
        "NSCameraUsageDescription": "Allow microphone features?",
        "NSMicrophoneUsageDescription": "Allow microphone features?"
      }
    },
    "android": {
      "versionCode": 1,
      "package": "com.killuhwhale.reptracker",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "backgroundColor": "#1e293b",
      "splash": {
        "image": "./assets/splash.png"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-9369132738006643~2252555759",
          "iosAppId": "ca-app-pub-9369132738006643~4687147405"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
